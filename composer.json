{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "artesaos/seotools": "^0.10.0",
        "askedio/laravel5-soft-cascade": "^2.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "6.5.x-dev",
        "imangazaliev/didom": "dev-master",
        "intervention/image": "^2.3",
        "jacquestvanzuydam/laravel-firebird": "dev-5.4-support",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "laravelrus/localized-carbon": "dev-master",
        "laravelrus/sleepingowl": "4.*@dev",
        "orchestra/parser": "~3.0",
        "ramsey/uuid": "3.x-dev",
        "renatomarinho/laravel-page-speed": "^1.8",
        "sidorovroman/file-site-options": "dev-master",
        "true/punycode": "~2.0",
        "zizaco/entrust": "dev-master"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php",
            "app/Helpers/CacheHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan optimize"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
